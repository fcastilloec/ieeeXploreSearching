{
  "manifest_version": 2,
  "name": "IEEE results to CSV",
  "version": "0.1.0",
  "description": "Download all IEEE results into a CSV file",
  "author": "Felipe Castillo",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_icon": {
      "16": "images/electricity16.png",
      "32": "images/electricity32.png",
      "48": "images/electricity48.png",
      "128": "images/electricity128.png"
    }
  },
  "icons": {
    "16": "images/electricity16.png",
    "32": "images/electricity32.png",
    "48": "images/electricity48.png",
    "128": "images/electricity128.png"
  },
  "permissions": ["activeTab", "declarativeContent"]
}
